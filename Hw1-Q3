#include <stdio.h>
#include <math.h>

int main() {
    int popularity, size;
    long homeValue;

    // Get input from the user
    printf("Enter popularity: ");
    scanf("%d", &popularity);
    
    printf("Enter size: ");
    scanf("%d", &size);

    // Compute home value
    homeValue = (pow(popularity, 3) + pow(size, 2)) * 10000;

    // Print the result
    printf("Home value is: %ld\n", homeValue);
    
    return 0;
}
/*  START
Enter popularity: 5
Enter size: 2
Home value is: 1290000
END  */
